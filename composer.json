{
  "name": "danielgoerz/forge-links",
  "type": "typo3-cms-extension",
  "description": "An extension to provide links to forge issues through the Link Wizard. And to demonstrate the usage of a newly introduced hook.",
  "authors": [
    {
      "name": "Daniel Goerz",
      "role": "Developer",
      "homepage": "https://usetypo3.com"
    }
  ],
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": ">=8.6.0,<9.3.0"
  },
  "autoload": {
    "psr-4": {
      "DanielGoerz\\ForgeLinks\\": "Classes"
    }
  },
  "replace": {
    "forge_links": "self.version"
  }
}
